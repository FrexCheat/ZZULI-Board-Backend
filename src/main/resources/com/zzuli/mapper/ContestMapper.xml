<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="zzuli.mapper.ContestMapper">
    <insert id="saveTeam" keyProperty="teamId">
        insert into teams ( team_name, team_coach, team_class,contest_id) values
        ( #{teamName}, #{teamCoach}, #{teamClass}, #{contestId});
    </insert>

    <insert id="saveMember">
        insert into members (team_id, contest_id,member_id,member_name,member_school,member_college,member_class,official) values
        (#{teamId}, #{contestId},#{memberId},#{memberName},#{memberSchool},#{memberCollege},#{memberClass},#{official});
    </insert>


    <select id="list" resultType="zzuli.pojo.entity.Contest">
        select * from contests;
    </select>
    <select id="getContestById" resultType="zzuli.pojo.entity.Contest">
        select * from contests where contest_id = #{id};
    </select>
</mapper>
